{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "adfvamsee"
		},
		"AzureSqlDWEmployee_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDWEmployee'"
		},
		"AzureSqlDatabase2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase2'"
		},
		"AzureSqlDatabaseemployee_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabaseemployee'"
		},
		"csvblobstorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'csvblobstorage'"
		},
		"csvlc_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'csvlc'"
		},
		"onpremsqlserver_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'onpremsqlserver'"
		},
		"sqldatabaselc_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'sqldatabaselc'"
		},
		"onpremsalesdata_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'onpremsalesdata'"
		},
		"Azurecustomersales_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Azurecustomersales'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDWEmployee_connectionString": {
						"value": "[parameters('AzureSqlDWEmployee_connectionString')]"
					},
					"AzureSqlDatabase2_connectionString": {
						"value": "[parameters('AzureSqlDatabase2_connectionString')]"
					},
					"AzureSqlDatabaseemployee_connectionString": {
						"value": "[parameters('AzureSqlDatabaseemployee_connectionString')]"
					},
					"csvblobstorage_connectionString": {
						"value": "[parameters('csvblobstorage_connectionString')]"
					},
					"csvlc_connectionString": {
						"value": "[parameters('csvlc_connectionString')]"
					},
					"sqldatabaselc_connectionString": {
						"value": "[parameters('sqldatabaselc_connectionString')]"
					},
					"Azurecustomersales_connectionString": {
						"value": "[parameters('Azurecustomersales_connectionString')]"
					},
					"onpremsqlserver_connectionString": {
						"value": "[parameters('onpremsqlserver_connectionString')]"
					},
					"onpremsalesdata_connectionString": {
						"value": "[parameters('onpremsalesdata_connectionString')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ArmTemplate_0')]"
			]
		}
	]
}