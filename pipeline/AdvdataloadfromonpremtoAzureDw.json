{
	"name": "AdvdataloadfromonpremtoAzureDw",
	"properties": {
		"activities": [
			{
				"name": "AdvLookup",
				"type": "Lookup",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlDWSource",
						"sqlReaderQuery": "Select Adventuretables from Adventuredata"
					},
					"dataset": {
						"referenceName": "Adventuretableset",
						"type": "DatasetReference"
					}
				}
			},
			{
				"name": "AdvForEach",
				"type": "ForEach",
				"dependsOn": [
					{
						"activity": "AdvLookup",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@activity('AdvLookup').output.value",
						"type": "Expression"
					},
					"isSequential": true,
					"activities": [
						{
							"name": "Adv Copy Data",
							"type": "Copy",
							"dependsOn": [],
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"source": {
									"type": "SqlServerSource",
									"sqlReaderQuery": {
										"value": "Select * from @{item().Adventuretables}",
										"type": "Expression"
									}
								},
								"sink": {
									"type": "SqlDWSink",
									"allowPolyBase": true,
									"polyBaseSettings": {
										"rejectValue": 0,
										"rejectType": "value",
										"useTypeDefault": false
									}
								},
								"enableStaging": false
							},
							"inputs": [
								{
									"referenceName": "Adventuredatasource",
									"type": "DatasetReference"
								}
							],
							"outputs": [
								{
									"referenceName": "Adventuredatasink",
									"type": "DatasetReference"
								}
							]
						}
					]
				}
			}
		],
		"annotations": []
	}
}